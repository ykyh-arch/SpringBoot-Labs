<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.iocoder.springboot.lab33.shirodemo.mapper.SysUserTokenMapper">

	<select id="queryByToken" resultType="cn.iocoder.springboot.lab33.shirodemo.dataobject.SysUserToken">
		select * from sys_user_token where token = #{value}
	</select>
</mapper>